{
    "name": "topRedis",
    "module" : "caf_components#supervisor",
    "description" : "Supervisor for redis",
    "env" : {
        "dieDelay" : -1,
        "interval" : 1000,
        "maxHangRetries": 10,
        "maxRetries" : 5,
        "retryDelay" : 100
    },
    "components":[
        {
            "name": "log",
            "module" : "caf_components#plug_log",
            "description" : "Logger",
            "env" : {
                "logLevel" : "TRACE"
            }
        },
        {
            "name": "cp",
            "module" : "../../index.js#plug",
            "description" : "redisPlug",
            "env" : {
               "nodeId": "sasdasd.vcap.me:4000",
               "paas" : "redis",
               "redis" : {
                   "password" : null,
                   "hostname" : "localhost",
                   "port" : 6379
                },
                "coalescing" : {
                    "interval" : 10,
                    "maxPendingUpdates" : 10
                }
            }
        },
        {
            "name": "cp2",
            "module" : "../../index.js#plug",
            "description" : "redisPlug",
            "env" : {
               "nodeId": "sasdasd.vcap.me:4001",
               "paas" : "redis",
               "redis" : {
                   "password" : null,
                   "hostname" : "localhost",
                   "port" : 6379
                },
                "coalescing" : {
                    "interval" : 10,
                    "maxPendingUpdates" : 10
                }
            }
        },
        {
            "name": "h2",
            "module" : "./ChildImpl",
            "description" : "Child2",
            "env" : {
                "message" : "child2",
                "number" : 9,
                "leaseTimeout": 1
            }
        },
        {
            "name": "h3",
            "module" : "./ChildImpl",
            "description" : "Child3",
            "env" : {
                "message" : "child3",
                "number" : 93,
                "leaseTimeout": 1
            }
        }
    ]
}
