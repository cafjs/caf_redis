{
    "name": "topRedis",
    "module" : "caf_components#supervisor",
    "description" : "Supervisor for redis",
    "env" : {
        "dieDelay" : -1,
        "interval" : 1000,
        "maxHangRetries": 10,
        "maxRetries" : 5,
        "retryDelay" : 100
    },
    "components":[
        {
            "name": "log",
            "module" : "caf_components#plug_log",
            "description" : "Logger",
            "env" : {
                "logLevel" : "DEBUG"
            }
        },
        {
            "name": "cp",
            "module" : "../../index.js#plug",
            "description" : "redisPlug",
            "env" : {
                "appName" : "foo-test",
               "nodeId": "sasdasd.vcap.me:4000",
               "paas" : "redis",
               "redis" : {
                   "password" : null,
                   "hostname" :  "process.env.REDIS_PORT_6379_TCP_ADDR||localhost",
                   "port" : 6379
                },
                "coalescing" : {
                    "interval" : 10,
                    "maxPendingUpdates" : 10
                },
                "compressState": "process.env.COMPRESS_STATE||true"
            }
        },
        {
            "name": "h2",
            "module" : "./ChildImpl",
            "description" : "Child2",
            "env" : {
