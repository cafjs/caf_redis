{
    "name": "topRedis",
    "module" : "caf_components#supervisor",
    "description" : "Supervisor for redis",
    "env" : {
        "dieDelay" : -1,
        "interval" : 1000,
        "maxHangRetries": 10,
        "maxRetries" : 5,
        "retryDelay" : 100
    },
    "components":[
        {
            "name": "log",
            "module" : "caf_components#plug_log",
            "description" : "Logger",
            "env" : {
                "logLevel" : "TRACE"
            }
        },
        {
            "name": "h1",
            "module" : "../../index.js#plug",
            "description" : "redisPlug",
            "env" : {
               "nodeId": "ssdsdasdsd_1",
               "redis" : {
                   "password" : null,
                   "host" : "localhost",
                   "port" : 6379
                },
                "coalescing" : {
                    "interval" : 10,
                    "maxPendingUpdates" : 10
                }
            }
        }
    ]
}
